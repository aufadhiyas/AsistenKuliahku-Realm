<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.citrafa.asistenkuliahku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="me.citrafa.asistenkuliahku.SessionManager.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/iconapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.frmLogin" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.frmDaftar" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.menuJadwalKuliah" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.menuJadwalUjian" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.menuJadwalLain" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.menuCatatan" />
        <activity
            android:name="me.citrafa.asistenkuliahku.ActivityClass.Dashboard"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.frmJadwalKuliah" />
        <activity android:name="me.citrafa.asistenkuliahku.ActivityClass.frmTugas" />
        <activity
            android:name="me.citrafa.asistenkuliahku.ActivityClass.menuJadwalKuliahTab"
            android:label="@string/title_activity_menu_jadwal_kuliah_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="me.citrafa.asistenkuliahku.frmJadwalPengganti"></activity>
        <service
            android:name=".SessionManager.FirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name=".SessionManager.FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>


</manifest>